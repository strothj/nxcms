{
  "presets": [
    "env",
    "stage-2",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "styled-jsx/babel",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "components": "./components",
        "^styles$": "./styles.js",
        "test-utils": "./test-utils"
      }
    }]
  ]
}
